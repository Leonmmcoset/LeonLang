require("basic")
// 测试字符串拼接功能

// 字符串与字符串拼接
var(a) = string:"Hello";
var(b) = string:" World!";
var(c) = var(a) + var(b);
basic.print(var(c))

// 字符串与整数拼接
var(d) = string:"Number: ";
